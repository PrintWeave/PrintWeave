{
  "version": "0.1.4",
  "npmClient": "npm",
  "command": {
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): publish %s",
      "changelogIncludeCommitsClientLogin": true,
      "generateReleaseNotes": true,
      "allowBranch": [
        "main",
        "beta",
        "develop"
      ],
      "remoteClient": "github"
    },
    "publish": {
      "conventionalCommits": true,
      "yes": true
    }
  },
  "packages": [
    "packages/*"
  ]
}
