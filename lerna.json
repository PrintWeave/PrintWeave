{
  "version": "0.0.20",
  "npmClient": "npm",
  "command": {
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): publish %s",
      "changelogIncludeCommitsClientLogin": true,
      "allowBranch": [
        "main"
      ],
      "remoteClient": "github"
    },
    "publish": {
      "conventionalCommits": true,
      "yes": true
    }
  },
  "packages": [
    "packages/*"
  ]
}
