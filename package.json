{
  "name": "printweave",
  "scripts": {
    "server:dev": "cd packages/printweave-api && npm run dev",
    "server:start": "cd packages/printweave-api && npm start",
    "server:watch": "cd packages/printweave-api && npm run watch",
    "server:build": "cd packages/printweave-api && npm run build",
    "web:dev": "cd packages/printweave-web && npm run dev",
    "web:start": "cd packages/printweave-web && npm run start",
    "web:watch": "cd packages/printweave-web && npm run watch",
    "web:build": "cd packages/printweave-web && npm run build",
    "api-types:build": "cd packages/printweave-api-types && npm run build",
    "api-types:watch": "cd packages/printweave-api-types && npm run watch",
    "printer-bambu:dev": "cd packages/bambu-printer && npm run watch",
    "printer-bambu:build": "cd packages/bambu-printer && npm run build",
    "models:build": "cd packages/printweave-models && npm run build",
    "models:watch": "cd packages/printweave-models && npm run watch",
    "dev": "concurrently -n server,web,api-types,printer-bambu,models -c magenta,blue,cyan,green,yellow \"npm run server:dev\" \"npm run web:dev\" \"npm run api-types:watch\" \"npm run printer-bambu:dev\" \"npm run models:watch\"",
    "watch": "concurrently -n server,web,api-types,printer-bambu,models -c magenta,blue,cyan,green,yellow \"npm run server:watch\" \"npm run web:watch\" \"npm run api-types:watch\" \"npm run printer-bambu:dev\" \"npm run models:watch\"",
    "start": "concurrently -n server,web -c magenta,blue \"npm run server:start\" \"npm run web:start\"",
    "build": "npm run api-types:build && npm run models:build && npm run printer-bambu:build && npm run server:build && npm run web:build",
    "configure": "cd packages/printweave-api && npm run configure",
    "test": "echo \"Error: no test specified\"",
    "new-version": "lerna version",
    "new-publish": "lerna publish from-package",
    "preview:new-version": "lerna version --dry-run",
    "run-tests": "lerna run test"
  },
  "author": "MMathijs",
  "repository": "https://github.com/PrintWeave/PrintWeave",
  "homepage": "https://github.com/PrintWeave/PrintWeave#readme",
  "bugs": {
    "url": "https://github.com/PrintWeave/PrintWeave/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^1.4.12",
    "@zip.js/zip.js": "^2.7.57",
    "basic-ftp": "^5.0.5",
    "concurrently": "^9.1.2",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "fast-xml-parser": "^5.2.1",
    "multer": "^2.0.0",
    "ssh2-sftp-client": "^12.0.0"
  },
  "devDependencies": {
    "@lerna-lite/list": "^4.3.0",
    "@lerna-lite/diff": "^4.3.0",
    "@lerna-lite/publish": "^4.3.0",
    "@lerna-lite/run": "^4.3.0",
    "@lerna-lite/version": "^4.3.0",
    "fs-extra": "^11.3.0"
  }
}
