{
  "name": "printweave",
  "version": "0.0.7",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:build": "cd server && npm run build",
    "dev": "concurrently -n server -c magenta,blue \"npm run server:dev\"",
    "start": "concurrently -n server -c magenta,blue \"npm run server:start\"",
    "build": "concurrently -n server -c magenta,blue \"npm run server:build\"",
    "configure": "cd server && npm run configure",
    "test": "echo \"Error: no test specified\"",
    "install:server": "cd server && npm install",
    "postinstall": "run install:server"
  },
  "author": "MMathijs",
  "repository": "https://github.com/PrintWeave/PrintWeave",
  "homepage": "https://github.com/PrintWeave/PrintWeave#readme",
  "bin": {
    "printweave": "./bin/cli.js"
  },
  "bugs": {
    "url": "https://github.com/PrintWeave/PrintWeave/issues"
  },
  "license": "MIT",
  "prepublish": "npm run build",
  "dependencies": {
    "commander": "^13.1.0",
    "concurrently": "^9.1.2",
    "shelljs": "^0.8.5"
  },
  "files": [
    "bin",
    "server/dist",
    "server/package.json"
  ]
}
