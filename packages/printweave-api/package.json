{
  "name": "@printweave/api",
  "main": "dist/start.js",
  "type": "module",
  "version": "0.5.0-beta.4",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/start.js",
    "dev": "nodemon --exec tsx src/start.ts",
    "watch": "tsc -w ",
    "configure": "node dist/configure.js",
    "migrate": "node dist/migrations.js migrate",
    "migrate:undo": "node dist/migrations.js rollback"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:PrintWeave/PrintWeave.git"
  },
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "./types"
    ]
  },
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.21",
    "@types/npm": "^7.19.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/validator": "^13.12.2",
    "@types/winston": "^2.4.4",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@printweave/api-types": "^0.5.0-beta.4",
    "@printweave/models": "^0.5.0-beta.4",
    "@types/cors": "^2.8.18",
    "bambu-node": "^3.22.21",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.10.3",
    "multer": "^2.0.0",
    "npm": "^11.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.2",
    "winston": "^3.17.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "tsx src/app.ts",
    "ignore": [
      "plugins/*package*.json",
      "**/*.js"
    ]
  }
}
